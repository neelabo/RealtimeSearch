<Window x:Class="NeeLaboratory.RealtimeSearch.Views.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NeeLaboratory.RealtimeSearch.Views"
        xmlns:converters="clr-namespace:NeeLaboratory.RealtimeSearch.Converters"
        xmlns:tres="clr-namespace:NeeLaboratory.RealtimeSearch.TextResource"
        mc:Ignorable="d"
        Title="{tres:TextBinding SettingWindow.Title}" Height="480" Width="800" MinHeight="360" MinWidth="500">

    <Window.CommandBindings>
        <CommandBinding Command="{x:Static local:SettingWindow.CloseCommand}" Executed="CloseCommand_Executed"/>
    </Window.CommandBindings>

    <local:NavigationView x:Name="NavicationView"/>

</Window>
